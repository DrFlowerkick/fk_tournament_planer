{
  "rust-analyzer.procMacro.ignored": {
    "leptos_macro": ["server"]
  },
  // Do NOT pass workspace-wide features to every package
  //"rust-analyzer.cargo.features": [],
  // Enable all features
  //"rust-analyzer.cargo.allFeatures": true,
  /*"rust-analyzer.rustfmt.overrideCommand": [
    "leptosfmt",
    "--stdin",
    "--rustfmt"
  ],*/
  "css.validate": false,
  "makefile.configureOnOpen": false,
  "tailwindCSS.includeLanguages": {
    "html": "html",
    "rust": "html"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.formatOnSave": true
  },
  "cSpell.language": "en,de-de",
  "cSpell.dictionaries": ["en", "de-de"],
  "cSpell.enabled": true,
  "cSpell.words": [
    "autoreload",
    "axum",
    "bindgen",
    "browserlist",
    "browserquery",
    "btrim",
    "buildx",
    "Buildx",
    "cdylib",
    "citext",
    "codegen",
    "daisyui",
    "drflowerkick",
    "dtolnay",
    "fieldset",
    "github",
    "GITHUB",
    "gloo",
    "Insertable",
    "leptos",
    "leptosfmt",
    "linux",
    "mousedown",
    "mouseenter",
    "pgcrypto",
    "plpgsql",
    "prefersdark",
    "rlib",
    "rustc",
    "srcset",
    "stylesheet",
    "Swatinem",
    "swiss",
    "taiki",
    "tailwindcss",
    "Timestamptz",
    "tokio"
  ],
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/{info,lfs,logs,refs,objects}/**",
    ".git/{index,*refs,*HEAD}",
    ".vscode",
    ".vscode-insiders",
    "**/*.lock",
    "LICENSE"
  ]
}
